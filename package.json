{
  "name": "kmd-idb",
  "version": "1.0.5",
  "description": "Angular2 Service for Accessing indexedDB",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "indexedDB",
    "Service"
  ],
  "author": "Moses Kabungo <kbng.moses@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.4.1",
    "loadash": "0.0.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.2",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4",

    "@angular/core": "~2.4.0",
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.0.10",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.15.1",
    "lodash": "^4.16.4",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "@types/node": "6.0.46",
    "@types/jasmine": "^2.5.36",
    "@types/lodash": "^4.14.44",

    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~4.0.14"
  }
}